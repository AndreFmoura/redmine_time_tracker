$(document).ready(function () {
    $('#tt_cb_show_all_my_bookings').click(function () {
        if ($('#tt_cb_show_all_my_bookings')[0].checked) {
            $.ajax({url:'tt_overview/show_all_my_logs',
                type:'PUT',
                success:function (transport) {
                    window.location.reload();
                }
            });
        } else {
            $.ajax({url:'tt_overview/hide_all_my_logs',
                type:'PUT',
                success:function (transport) {
                    window.location.reload();
                }
            });
        }
    });
});
