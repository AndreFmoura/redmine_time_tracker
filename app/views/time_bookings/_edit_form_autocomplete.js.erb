<% unless time_booking.nil? %>

$(function () {
    $('#time_booking_edit_<%= time_booking.id.to_s %>_issue_id').autocomplete({
        source:'tt_completer/get_issue.json?key=<%= key %>',
        minLength:0,
        select:function (event, ui) {
            $('#time_booking_edit_<%= time_booking.id.to_s %>_issue_id').val(ui.item.value);
            redmine_time_tracker.TimeTracker.updateBookingProject('<%= key %>', 'time_booking_edit_<%= time_booking.id.to_s %>');
        }
    });
});

$(document).ready(function() {
    redmine_time_tracker.TimeTracker.validate_list_inputs('time_booking_edit_<%= time_booking.id.to_s %>');
});
<% end %>